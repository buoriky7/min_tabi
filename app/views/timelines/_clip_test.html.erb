<% if user_signed_in? %>
	<% if timeline.cliped_by?(current_user) %>
	<div id="clip-btn-js<%= timeline.id %>">
		<%= link_to test_clip_d_path(timeline_id: timeline.id), method: :delete, remote: true do %>
			<!-- <span class="glyphicon glyphicon-star" style="color: #ccc;" aria-hidden="true">いいね済</span> -->
			<span class="glyphicon glyphicon-paperclip clip-btn clipped" aria-hidden="true"></span>
		<% end %>
		</div>
	<% else %>
	<div id="clip-btn-js<%= timeline.id %>">
		<%= link_to test_clip_path(timeline.id), method: :post, remote: true do %>
			<!-- <span class="glyphicon glyphicon-star-empty" aria-hidden="true">いいね！</span> -->
			<span class="glyphicon glyphicon-paperclip clip-btn" aria-hidden="true"></span>
		<% end %>
	</div>
	<% end %>
<% end %>

